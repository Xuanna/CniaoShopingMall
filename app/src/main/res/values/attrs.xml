<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="CnToolbar">

        <attr name="rightButtonIcon" format="reference"/>
        <attr name="isShowSearchView" format="boolean"/>
        <attr name="myTitleText" format="string|reference"/>
    </declare-styleable>

    <declare-styleable name="ClearEditText">
        <attr name="drawableWidth" format="integer"/>
        <attr name="drawableHeight" format="integer"/>
        <attr name="deleteImgIcon" format="reference"/>
    </declare-styleable>
    
    <declare-styleable name="NumberAddView">
        <attr name="value" format="integer|reference"/>
        <attr name="minValue" format="integer|reference"/>
        <attr name="maxValue" format="integer|reference"/>
        <attr name="btnAddBackground" format="reference"/>
        <attr name="btnReduceBackground" format="reference"/>
        <attr name="textViewBackground" format="reference"/>
    </declare-styleable>

</resources>